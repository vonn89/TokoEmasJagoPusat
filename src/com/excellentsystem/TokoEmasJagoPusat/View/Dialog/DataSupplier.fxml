<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="400.0" styleClass="dialog" stylesheets="@../../Resource/MyTheme.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="com.excellentsystem.TokoEmasJagoPusat.View.Dialog.DataSupplierController">
   <children>
      <GridPane hgap="7.0" layoutX="130.0" layoutY="67.0" vgap="7.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="5.0" minHeight="5.0" prefHeight="5.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
         <children>
            <TableView fx:id="supplierTable" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true" GridPane.rowIndex="3">
              <columns>
                <TableColumn fx:id="kodeSupplierColumn" prefWidth="75.0" text="Kode Supplier" />
                  <TableColumn fx:id="removeColumn" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Label styleClass="label-header" text="Data Supplier" />
            <Button mnemonicParsing="false" onAction="#close" prefHeight="30.0" prefWidth="30.0" GridPane.halignment="RIGHT">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Resource/close.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.rowIndex="2">
               <children>
                  <TextField fx:id="kodeSupplierField" maxWidth="1.7976931348623157E308" prefHeight="30.0" promptText="kode supplier" HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" onAction="#saveSupplier" prefHeight="30.0" prefWidth="80.0" text="Save" />
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
